{
    "manifest_version": 3,
    "version": "1.0.0",
    "name": "Hide Dollar",
    "description": "$を見ることはもう無いだろう",
    "background": {
        "service_worker": "hide-dollar.js"
    },
    "action": {
        "default_title": "Hide dollar"
    },
    "icons": {
        "16":"icon/icon_16x16.png",
        "48": "icon/icon_48x48.png",
        "128": "icon/icon_128x128.png"
    },
    "permissions": ["activeTab", "scripting"]
}
